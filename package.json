{
  "name": "jsreport-public-templates",
  "version": "1.1.2",
  "description": "jsreport extension for granting public access to particular templates",
  "scripts": {
    "test": "mocha test --timeout=5000",
    "build": "npm run prepublish",
    "prepublish": "in-publish && jsreport-studio-build || not-in-publish"
  },
  "homepage": "https://github.com/jsreport/jsreport-public-templates",
  "repository": {
    "type": "git",
    "url": "git@github.com:jsreport/jsreport-public-templates.git"
  },
  "keywords": [
    "jsreport"
  ],
  "standard": {
    "ignore": [
      "main.js"
    ],
    "parser": "babel-eslint",
    "env": {
      "node": true,
      "mocha": true,
      "browser": true
    }
  },
  "license": "LGPL",
  "dependencies": {
    "bluebird": "3.5.0",
    "string": "3.3.3",
    "underscore": "1.8.3",
    "uuid": "2.0.2"
  },
  "author": "Jan Blaha",
  "devDependencies": {
    "babel-eslint": "6.0.4",
    "eslint": "2.11.1",
    "eslint-config-standard-jsx": "1.2.1",
    "eslint-plugin-babel": "3.2.0",
    "in-publish": "2.0.0",
    "jsreport-authentication": "jsreport/jsreport-authentication#2.0.0-beta",
    "jsreport-authorization": "jsreport/jsreport-authorization#2.0.0-beta",
    "jsreport-core": "jsreport/jsreport-core#2.0.0-beta",
    "jsreport-express": "jsreport/jsreport-express#2.0.0-beta",
    "jsreport-studio-dev": "1.1.0",
    "jsreport-templates": "jsreport/jsreport-templates#2.0.0-beta",
    "mocha": "2.5.3",
    "should": "9.0.1",
    "standard": "7.1.2",
    "supertest": "1.2.0"
  },
  "files": [
    "lib",
    "studio",
    "index.js",
    "jsreport.config.js"
  ],
  "engines": {
    "node": ">=8.0"
  }
}
