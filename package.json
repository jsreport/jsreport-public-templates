{
  "name": "jsreport-public-templates",
  "version": "2.0.0",
  "description": "jsreport extension for granting public access to particular templates",
  "scripts": {
    "test": "mocha test --timeout=5000",
    "start": "jsreport-studio-start --httpPort 5488",
    "build": "jsreport-studio-build",
    "prepublish": "in-publish && jsreport-studio-build || not-in-publish"
  },
  "homepage": "https://github.com/jsreport/jsreport-public-templates",
  "repository": {
    "type": "git",
    "url": "git@github.com:jsreport/jsreport-public-templates.git"
  },
  "keywords": [
    "jsreport"
  ],
  "standard": {
    "ignore": [
      "main.js"
    ],
    "parser": "babel-eslint",
    "env": {
      "node": true,
      "mocha": true,
      "browser": true
    }
  },
  "license": "LGPL",
  "dependencies": {
    "uuid": "3.2.1"
  },
  "author": "Jan Blaha",
  "devDependencies": {
    "babel-eslint": "8.2.3",
    "eslint": "4.19.1",
    "eslint-config-standard-jsx": "5.0.0",
    "eslint-plugin-babel": "5.1.0",
    "in-publish": "2.0.0",
    "jsreport-authentication": "2.0.0",
    "jsreport-authorization": "2.0.0",
    "jsreport-core": "2.0.0",
    "jsreport-express": "2.0.0",
    "jsreport-studio-dev": "1.1.2",
    "jsreport-templates": "2.0.0",
    "mocha": "5.1.1",
    "should": "13.2.1",
    "standard": "11.0.1",
    "supertest": "3.0.0"
  },
  "files": [
    "lib",
    "studio",
    "index.js",
    "jsreport.config.js"
  ],
  "engines": {
    "node": ">=8.9"
  }
}
